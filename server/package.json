{
  "name": "liph-bank.server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:production": "node dist/main",
    "prisma:generate": "prisma generate dev",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:build": "prisma generate",
    "prisma:validate": "prisma format",
    "prisma:pull": "prisma introspect",
    "prisma:seed": "prisma db seed",
    "prisma:generate:sql": "prisma migrate diff --from-empty --to-schema-datasource database/schema.prisma --script > database/generated.sql",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "vitest run --config ./config/test/vitest/vite.config.ts",
    "test:watch": "vitest --config ./config/test/vitest/vitest.config.ts",
    "test:cov": "vitest run --coverage --config ./config/test/vitest/vite.config.ts",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false --config ./config/test/vitest/vite.config.ts",
    "test:e2e": "vitest run --config ./config/test/vitest/vitest.config.e2e.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.13",
    "@nestjs/core": "^10.4.13",
    "@nestjs/platform-express": "^10.4.13",
    "@prisma/client": "^6.0.1",
    "bcrypt": "^5.1.1",
    "liph-bank.server": "file:",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.13",
    "@swc/core": "^1.10.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "prisma": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "unplugin-swc": "^1.5.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "schema": "database/schema.prisma"
  }
}